{
  "name": "lab-vue-form-validate",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dev:host": "vite --host --open",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "format": "prettier --cache --write \"src/**/*.{vue,ts}\"",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore"
  },
  "lint-staged": {
    "*src/**/*.{js,ts,vue}": [
      "eslint --fix --ignore-path .gitignore",
      "prettier --write"
    ],
    "*src/**/*.{css,scss,vue}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@vee-validate/zod": "^4.7.3",
    "maska": "^2.1.3",
    "vee-validate": "^4.7.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "yup": "^0.32.11",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.1.3",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@unocss/preset-icons": "^0.49.4",
    "@unocss/preset-rem-to-px": "^0.49.4",
    "@unocss/reset": "^0.49.4",
    "@unocss/transformer-variant-group": "^0.49.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.5",
    "unocss": "^0.49.4",
    "vite": "^4.1.1",
    "vite-plugin-vue-inspector": "^3.0.0-beta.0",
    "vue-tsc": "^1.0.12"
  }
}
